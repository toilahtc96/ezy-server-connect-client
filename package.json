{
  "name": "cv-client",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:development": "vue-tsc --noEmit && vite build --mode development",
    "build:staging": "vue-tsc --noEmit && vite build --mode staging",
    "preview": "vite preview",
    "types-check": "vue-tsc --noEmit",
    "lint:script": "eslint -c .eslintrc.js \"./src/**/*.{js,ts,vue}\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check \"src/**/*+(js|ts|vue|scss)\"",
    "lint:style": "stylelint src/**/*.{css,scss}",
    "lint": "npm run lint:script && npm run lint:prettier && npm run lint:style",
    "lintfix": "prettier --write --list-different \"src/**/*+(js|ts|vue|scss)\" && npm run lint:script -- --fix && npm run lint:style -- --fix"
  },
  "dependencies": {
    "ant-design-vue": "^3.2.10",
    "axios": "^0.27.2",
    "lodash": "^4.17.21",
    "vue": "^3.2.37",
    "@ant-design/icons-vue": "^6.1.0",
    "date-fns": "^2.28.0",
    "pinia": "^2.0.13",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.181",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "babel-plugin-import": "^1.13.5",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.2.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "sass": "^1.50.0",
    "lint-staged": "^12.3.7",
    "postcss-html": "^1.3.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-csstree-validator": "^2.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.7.4",
    "unplugin-vue-components": "^0.19.3",
    "vite": "^3.0.0",
    "ant-design-vue": "^3.2.10",
    "axios": "^0.27.2",
    "lodash": "^4.17.21",
    "vue-tsc": "^0.38.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue,css,scss}": "npm run lint"
  }
}
